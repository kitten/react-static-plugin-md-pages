{
  "name": "react-static-plugin-md-pages",
  "description": "react-static plugin to create nested pages from a given source directory",
  "version": "0.3.1",
  "main": "index.js",
  "license": "MIT",
  "bugs": "https://github.com/kitten/react-static-plugin-md-pages/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/kitten/react-static-plugin-md-pages.git"
  },
  "scripts": {
    "build": "babel src --out-dir .",
    "clean": "rimraf *.js",
    "prepublishOnly": "run-s clean build"
  },
  "files": [
    "README.md",
    "LICENSE",
    "*.js"
  ],
  "babel": {
    "presets": [
      "react-static/babel-preset"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "lint-staged": {
    "*.{js,jsx,json,md}": "prettier --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --quiet --relative"
    }
  },
  "peerDependencies": {
    "react-static": ">=7.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "hast-util-raw": "^5.0.2",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "react-static": "^7.2.3",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.5.5",
    "@mdx-js/react": "^1.5.5",
    "github-slugger": "^1.2.1",
    "glob": "^7.1.6",
    "loader-utils": "^1.4.0",
    "mdast-util-to-string": "^1.0.7",
    "remark": "^11.0.2",
    "remark-frontmatter": "^1.3.2",
    "remark-squeeze-paragraphs": "^3.0.4",
    "to-vfile": "^6.0.0",
    "unist-util-remove": "^2.0.0",
    "unist-util-select": "^3.0.1",
    "unist-util-visit": "^2.0.2",
    "yaml": "^1.7.2"
  }
}
